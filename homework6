# part1
my_dict = {'Alex': 2002, 'Jo': 2000, 'Den': 1999}
print('Original dict:', my_dict)
print('Existing value:', my_dict['Alex'])
print('Not existing value:', my_dict.get('Lesha', 'Not found'))
my_dict.update({'Misha': 1998, 'Lera': 1255})
Jo = my_dict.pop('Jo')
print('Deleted value', Jo)
print('Modified dict', my_dict)

# part2
my_set = {1, 1, 1, 2, 3, 33, 33, 4, 5, 5, 5, 6}
print('Set', my_set)
my_set.add('Apple')
my_set.add((1, 5, 6))
my_set.remove(1)
print('Modified set', my_set)
